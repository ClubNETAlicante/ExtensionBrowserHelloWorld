{
	"name": ".Net Alicante prueba",
	"version": "1",
	"manifest_version": 2,
	"description": "Charla Club .NET",
	"icons": { "128": "media/icon128.png" },
    "permissions": [
		"tabs"
	],
	"content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery.min.js",
                "inject.js"
            ],
            "css": [
                "inject.css"
            ]
        }
    ],
	"browser_action": {
		"default_popup": "popup/popup.html"
	}
}